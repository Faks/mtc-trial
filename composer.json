{
    "name": "solumdesignum/slim-skeleton-mvc",
    "description": "Simple Slim Framework 4 skeleton with Blade, Monolog, Eloquent, Middleware.",
    "license": "MIT",
    "type": "project",
    "keywords": [
        "slim-framework",
        "skeleton",
        "mvc",
        "blade",
        "monolog",
        "eloquent",
        "middleware"
    ],
    "authors": [
        {
            "name": "Oskars Germovs",
            "email": "oskars_germovs@inbox.lv",
            "homepage": "https://solum-designum.eu"
        },
        {
            "name": "Nathanael Semhoun",
            "email": "nathanael@semhoun.net",
            "homepage": "http://www.semhoun.net"
        }
    ],
    "require": {
        "php": ">=5.5.0|^7.4|^8.0",
        "slim/slim": "^4.8",
        "slim/php-view": "^3.1.0",
        "robmorgan/phinx": "^0.12.7",
        "ezyang/htmlpurifier": "^v4.13.0",
        "intervention/image": "^2.5.1",
        "slim/csrf": "^1.2.1",
        "peppeocchi/php-cron-scheduler": "^v2.4.1",
        "ext-pdo": "*",
        "jenssegers/blade": "^1.4",
        "slim/psr7": "^1.3",
        "monolog/monolog": "^2.3",
        "php-di/php-di": "^6.3",
        "illuminate/database": "^v8.49.2",
        "illuminate/pagination": "v8.49.2",
        "illuminate/translation": "^v8.49.2",
        "illuminate/validation": "^v8.49.2",
        "symfony/var-dumper": "^v5.3.0",
        "ext-curl": "*"
    },
    "require-dev": {
        "jangregor/phpstan-prophecy": "^0.8.1",
        "phpstan/extension-installer": "^1.1.0",
        "phpstan/phpstan": "^0.12.80",
        "phpunit/phpunit": "^8.0 || ^9.0"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "src\\": "src/",
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }

}
